{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/apollo",
      "vue-apollo",
      "vue-cli-plugin-apollo"
    ],
    "typeRoots": ["types", "node_modules/@types"]
  },
  "exclude": [
    "node_modules"
  ],
  "rules": {
    "no-console": [
      false
    ]
  }
}